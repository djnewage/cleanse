{
  "name": "cleanse",
  "version": "1.4.3",
  "description": "A desktop app for censoring profanity in audio files",
  "main": "./out/main/index.js",
  "author": "",
  "homepage": "https://electron-vite.org",
  "scripts": {
    "format": "prettier --write .",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:backend": "bash scripts/build-backend.sh",
    "build:mac": "npm run build:backend && electron-vite build && electron-builder --mac",
    "build:mac:dev": "npm run build:backend && electron-vite build && electron-builder --mac --arm64",
    "build:win": "npm run build && electron-builder --win",
    "build:linux": "electron-vite build && electron-builder --linux"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.2",
    "@electron-toolkit/utils": "^4.0.0",
    "electron-log": "^5.4.3",
    "electron-updater": "^6.7.3",
    "firebase": "^10.14.0"
  },
  "devDependencies": {
    "@electron-toolkit/tsconfig": "^2.0.0",
    "@electron/notarize": "^3.1.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^22.10.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.3",
    "electron": "^33.2.0",
    "electron-builder": "^26.7.0",
    "electron-vite": "^5.0.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.7.0",
    "vite": "^6.0.0"
  }
}
